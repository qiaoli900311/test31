{
  "name": "deepexi-eggjs",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.0",
    "koa-logger": "^3.2.0",
    "koa-compress": "2.0.0",
    "lodash": "^4.17.4",
    "egg-validate-plus": "^1.1.6",
    "deepexi-body": "^0.0.1",
    "deepexi-onerror": "^0.2.0",
    "@taccisum/egg-utils": "^0.1.0",
    "@taccisum/egg-eureka": "^1.0.1",
    "egg-swagger-doc": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "husky": "^2.7.0",
    "standard-version": "^8.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --env=${ENV:-prod} --port=${PORT:-8080} --workers=${WORKERS:-4} --title=deepexi-eggjs",
    "stop": "egg-scripts stop --title=deepexi-eggjs",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "commit": "npx git-cz",
    "release": "standard-version",
    "autod": "autod",
    "postinstall": "npm link ./modules/domain"
  },
  "ci": {
    "version": "10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
